<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
  <classPathEntry location="D:/ProgramData/.m2/repository/mysql/mysql-connector-java/5.1.30/mysql-connector-java-5.1.30.jar" />
  <context id="context1" >
  	<!-- 去除自动生成的注释 -->
	<commentGenerator>
		<property name="suppressAllComments" value="true" />
	</commentGenerator>
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" 
    	connectionURL="jdbc:mysql://192.168.0.158:3306/physical" userId="root" password="12345678" />
    <!--  默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer   
         true，把JDBC DECIMAL 和 NUMERIC 类型解析为java.math.BigDecimal   
     -->   
	<javaTypeResolver >  
	    <property name="forceBigDecimals" value="false" />  
	</javaTypeResolver> 
    <javaModelGenerator targetPackage="com.me.report.model" targetProject="me-Report">
    	<!-- enableSubPackages:是否让数据库名作为包的后缀 --> 
    	<property name="enableSubPackages" value="false" />
    </javaModelGenerator>
    <sqlMapGenerator targetPackage="com.me.report.dao.mapper" targetProject="me-Report">
    	<property name="enableSubPackages" value="false" />
    </sqlMapGenerator>
    <javaClientGenerator targetPackage="com.me.report.dao" targetProject="me-Report" type="XMLMAPPER">
    	<property name="enableSubPackages" value="false" />
    </javaClientGenerator>
    <!-- 如果table里边不配置property，默认字段都生成为类属性。  -->
    <!-- 忽略字段 
	<ignoreColumn column="FRED" /> -->
	<!-- schema:数据库名；tableName:用于自动生成代码的数据库表；domainObjectName:对应于数据库表的javaBean类名 -->
	
    <table schema="" tableName="physical" domainObjectName="PhysicalModel" enableCountByExample="false" 
    	enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" 
    	selectByExampleQueryId="false" />
    <table schema="" tableName="information" domainObjectName="InformationModel" enableCountByExample="false" 
    	enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" 
    	selectByExampleQueryId="false" />
    <table schema="" tableName="suggest" domainObjectName="SuggestModel" enableCountByExample="false" 
    	enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" 
    	selectByExampleQueryId="false" />
    <table schema="" tableName="summary" domainObjectName="SummaryModel" enableCountByExample="false" 
    	enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" 
    	selectByExampleQueryId="false" />
  </context>
</generatorConfiguration>